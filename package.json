{
  "name": "suncalc-wasm",
  "version": "1.0.33",
  "sideEffects": false,
  "types": "./lib/rlease.d.ts",
  "main": "./lib/release.js",
  "scripts": {
    "test": "node tests",
    "asbuild:debug": "asc assembly/index.ts --target debug",
    "asbuild:release": "asc assembly/index.ts --target release",
    "asbuild": "npm run asbuild:debug && npm run asbuild:release",
    "start": "npx serve ."
  },
  "author": "BadMachine",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/node": "^20.9.2",
    "assemblyscript": "^0.27.19"
  },
  "files": [
    "lib/release.wasm",
    "lib/release.js",
    "lib/release.d.ts"
  ],
  "keywords": [
    "wasm",
    "suncalc",
    "suncalc",
    "webassebly"
  ],
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/release.js",
      "types": "./lib/release.d.ts"
    }
  }
